  <div class="contents row" >
    <% @outgoings.each do |outgoing| %>
      <p><%= outgoing.item %></p>
      <p><%= outgoing.price %></p>
      <p><%= outgoing.place %></p>
      <p><%= outgoing.date %></p>
      <div class="more">
        <ul class="more_list">
          <li>
            <%= link_to '編集', "/outgoings/#{outgoing.id}/edit", method: :get %>
          </li>
          <li>
            <%= link_to '削除', "/outgoings/#{outgoing.id}", method: :delete %>
          </li>
        </ul>
      </div>
      <hr>
    <% end %>
  </div>

